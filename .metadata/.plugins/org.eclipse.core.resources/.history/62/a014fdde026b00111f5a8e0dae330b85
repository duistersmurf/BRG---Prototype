package model;

public class Value 
{
	private int ID;
	private Object value;
	private String valueString;
//	private boolean isTable;
	
	public Value(int _ID, Object _value)
	{
		ID = _ID;
		setValue(_value);
		setValueString(_value);
	}

	public int getID()
	{
		return ID;
	}
	
	public String getValueString()
	{
		return valueString;
	}
	
	public Object getValue()
	{
		return value;
	}
	
	private void setValue(Object _value) 
	{
		value = _value;
	}
	
}
