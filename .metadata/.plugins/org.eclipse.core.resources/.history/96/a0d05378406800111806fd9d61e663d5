package model;

public class Database 
{
	private int ID;
	private String dbType;
	private String hostName;
	private String name;
	private String password;
	private int port;
	
	
	

	public int getID()
	{
		return ID;
	}




	public void setDbType(String dbType) {
		this.dbType = dbType;
	}




	public String getDbType() {
		return dbType;
	}
}
